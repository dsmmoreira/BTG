tKnights.vue","sourceRoot":"","sourcesContent":["<template>\n    <div>\n        <form @submit.prevent=\"updateName\">\n            <div class=\"card\">\n                <div class=\"card-header\">\n                    <h2>Alterar Nome</h2>\n                </div>\n                <div class=\"card-body\">\n                    <div class=\"form-row\">\n                        <div class=\"form-group col-md-12\">\n                            <label for=\"nickname\" class=\"form-label\">Novo Apelido:</label>\n                            <input type=\"text\" id=\"nickname\" v-model=\"newnickname\" class=\"form-control\" placeholder=\"Apelido\">\n                        </div>\n                        <div class=\"form-row\">\n                            <div class=\"form-group col-md-3\">\n                                <button type=\"submit\" class=\"btn btn-primary\">Alterar</button>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </form>\n    </div>\n</template>\n\n<script>\n    import axios from 'axios';\n\n    export default {\n        data() {\n            return {\n                newnickname: '',\n            }\n        },\n        methods: {\n            async updateName() {\n                try {\n\n                    await axios.put(`/api/Knights/Edit/${this.$route.params.id}`, {\n                        nickname: this.newnickname,\n                    });\n                    this.$router.push('/ListKnights');\n                } catch (error) {\n                    console.error(error);\n                }\n            },\n        },\n    }\n</script>\n"]}]}