;\n                } catch (error) {\n                    console.error(error);\n                }\n            },\n            GetAll() {\n                axios.get('/api/Knights/GetAllKnights')\n                    .then((response) => {\n                        this.knights = response.data;\n                    })\n                    .catch(function (error) {\n                        alert(error);\n                    });\n            },\n            goToDetails(id) {\n                this.$router.push(`/DetailsKnight/${id}`);\n            },\n            goToEditNickname(id) {\n                this.$router.push(`/EditKnights/${id}`);\n            }\n        },\n        mounted() {\n            this.GetAll();\n        }\n    }\n</script>"],"mappings":";gCACIA,mBAAA,CAAuC;EAAnCC,EAAE,EAAC;AAAY,GAAC,gBAAc;;;;gCAEfD,mBAAA,CAAmB,YAAf,YAAU;oBAAdE,UAAmB,C;;;EAE/BC,KAAK,EAAC,qBAAqB;EAAC,iBAAe,EAAC;;gCAC/CH,mBAAA,CAYQ,gB,aAXJA,mBAAA,CAUK,a,aATDA,mBAAA,CAAa,YAAT,MAAI,G,aACRA,mBAAA,CAAc,YAAV,OAAK,G,aACTA,mBAAA,CAAgB,YAAZ,SAAO,G,aACXA,mBAAA,CAAyB,YAArB,kBAAgB,G,aACpBA,mBAAA,CAAe,YAAX,QAAM,G,aACVA,mBAAA,CAAoB,YAAhB,aAAW,G,aACfA,mBAAA,CAAkB,YAAd,WAAS,G,aACbA,mBAAA,CAAwB,YAApB,iBAAe,G,aACnBA,mBAAA,CAAgB,YAAZ,SAAO,E;;;;;6DAfvBI,UAAuC,E,CAE7BC,KAAA,CAAAC,OAAO,I,cAAjBC,mBAAA,CAA0C,KAAAC,UAAA,MAAAC,UAAA,M,mCAE4BJ,KAAA,CAAAC,OAAO,I,cAA7EC,mBAAA,CA2BQ,SA3BRG,UA2BQ,GA1BJC,UAYQ,EACRX,mBAAA,CAYQ,iB,kBAXJO,mBAAA,CAUKK,SAAA,QAAAC,WAAA,CAVgBR,KAAA,CAAAC,OAAO,EAAjBQ,MAAM;yBAAjBP,mBAAA,CAUK;MAVgCQ,GAAG,EAAED;IAAM,IAC5Cd,mBAAA,CAA0B,YAAAgB,gBAAA,CAAnBF,MAAM,CAACG,IAAI,kBAClBjB,mBAAA,CAA2B,YAAAgB,gBAAA,CAApBF,MAAM,CAACI,KAAK,kBACnBlB,mBAAA,CAA6B,YAAAgB,gBAAA,CAAtBF,MAAM,CAACK,OAAO,kBACrBnB,mBAAA,CAA2B,YAAAgB,gBAAA,CAApBF,MAAM,CAACM,KAAK,kBACnBpB,mBAAA,CAA4B,YAAAgB,gBAAA,CAArBF,MAAM,CAACO,MAAM,kBACpBrB,mBAAA,CAAiC,YAAAgB,gBAAA,CAA1BF,MAAM,CAACQ,WAAW,kBACzBtB,mBAAA,CAAsG,aAAlGA,mBAAA,CAA6F;MAArFuB,IAAI,EAAC,QAAQ;MAACpB,KAAK,EAAC,cAAc;MAAEqB,OAAK,EAAAC,MAAA,IAAEC,QAAA,CAAAC,WAAW,CAACb,MAAM,CAACb