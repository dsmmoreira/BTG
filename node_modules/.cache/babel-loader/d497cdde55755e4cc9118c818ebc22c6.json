     <th>Apelido</th>\n                <th>Qauntidade Armas</th>\n                <th>Ataque</th>\n                <th>Experiencia</th>\n                <th>Consultar</th>\n                <th>Alterar Apelido</th>\n                <th>Excluir</th>\n            </tr>\n        </thead>\n        <tbody>\n            <tr v-for=\"knight of knights\" v-bind:key=\"knight\">\n                <td>{{ knight.nome }}</td>\n                <td>{{ knight.idade }}</td>\n                <td>{{ knight.apelido }}</td>\n                <td>{{ knight.armas }}</td>\n                <td>{{ knight.ataque }}</td>\n                <td>{{ knight.experiencia }}</td>\n                <td><button type=\"button\" class=\"btn btn-info\" @click=\"goToDetails(knight.id)\">Consultar</button></td>\n                <td><button type=\"button\" class=\"btn btn-warning\" @click=\"goToEditNickname(knight.id)\">Alterar Apelido</button></td>\n                <td><button type=\"button\" class=\"btn btn-danger\" @click=\"deleteKnight(knight.id)\">Excluir</button></td>\n            </tr>\n        </tbody>\n    </table>\n</template>\n\n\n<script>\n    import axios from 'axios'\n    export default {\n        name: \"ListKnights\",\n        data() {\n            return {\n                knights: []\n            }\n        },\n        methods: {\n            async deleteKnight(id) {\n                try {\n                    await axios.delete(`/api/Knights/Delete/${id}`);\n                    this.knights = this.knights.filter(knight => knight.id !== id);\n                } catch (error) {\n                    console.error(error);\n                }\n            },\n            GetAll() {\n                axios.get('/api/Knights/GetAllKnights')\n                    .then((response) => {\n                        this.knights = response.data;\n                    })\n                    .catch(function (error) {\n                        alert(error);\n                    });\n            },\n            goToDetails(id) {\n                this.$router.push(`/View/${id}`);\n            },\n            goToEditNickname(id) {\n                this.$router.push(`/EditKnights/${id}`);\n            }\n        },\n        mounted() {\n            this.GetAll();\n        }\n    }\n</script>"],"mappings":"AAqCI,OAAOA,KAAI,MAAO,OAAM;AACxB,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,OAAO,EAAE;IACb;EACJ,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,YAAYA,CAACC,EAAE,EAAE;MACnB,IAAI;QACA,MAAMN,KAAK,CAACO,MAAM,CAAE,uBAAsBD,EAAG,EAAC,CAAC;QAC/C,IAAI,CAACH,OAAM,GAAI,IAAI,CAACA,OAAO,CAACK,MAAM,CAACC,MAAK,IAAKA,MAAM,CAACH,EAAC,KAAMA,EAAE,CAAC;MAClE,EAAE,OAAOI,KAAK,EAAE;QACZC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACxB;IACJ,CAAC;IACDE,MAAMA,CAAA,EAAG;MACLZ,KAAK,CAACa,GAAG,CAAC,4BAA4B,EACjCC,IAAI,CAAEC,QAAQ,IAAK;QAChB,IAAI,CAACZ,OAAM,GAAIY,QAAQ,CAACb,IAAI;MAChC,CAAC,EACAc,KAAK,CAAC,UAAUN,KAAK,EAAE;QACpBO,KAAK,CAACP,KAAK,CAAC;MAChB,CAAC,CAAC;IACV,CAAC;IACDQ,WAAWA,CAACZ,EAAE,EAAE;MACZ,IAAI,CAACa,OAAO,CAACC,IAAI,CAAE,SAAQd,EAAG,EAAC,CAAC;IACpC,CAAC;IACDe,gBAAgBA,CAACf,EAAE,EAAE;MACjB,IAAI,CAACa,OAAO,CAACC,IAAI,CAAE,gBAAed,EAAG,EAAC,CAAC;IAC3C;EACJ,CAAC;EACDgB,OAAOA,CAAA,EAAG;IACN,IAAI,CAACV,MAAM,CAAC,CAAC;EACjB;AACJ","ignoreList":[]}]}